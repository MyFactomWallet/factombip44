{
  "name": "factombip44",
  "version": "0.1.7",
  "description": "Bip44 implmentation for Factom. Just generates private keys",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:node",
    "example": "browserify examples/examples.js -s Util -t [ babelify --presets [ es2015 react ] ] > dist/examples.js && node dist/examples.js",
    "test:node": "istanbul test mocha -- --reporter spec",
    "prepublish": "npm run test && npm run build:docs && npm run build:dist",
    "coveralls": "npm run coverage && coveralls <coverage/lcov.info",
    "build:docs": "documentation --github  -f md ./index.js > ./docs/index.md",
    "build:dist": "browserify -r ./index.js -s Util -t [ babelify --presets [ es2015 react ] ] > dist/factombip44.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MyFactomWallet/factombip44.git"
  },
  "keywords": [
    "factom",
    "utilties"
  ],
  "author": "emyrk",
  "license": "MIT",
  "dependencies": {
    "bip32": "^1.0.2",
    "bip32-utils": "^0.11.1",
    "bip39": "^2.5.0",
    "bip44-constants": "^7.1.0",
    "factom": "^1.0.0-rc4",
    "safe-buffer": "^5.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "documentation": "^9.1.1",
    "istanbul": "^0.4.5",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "standard": "*",
    "uglifyify": "^5.0.1",
    "browserify": "^16.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "factom-vote": "^0.2.0"
  }
}
